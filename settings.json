{
    //#region telemetry
    // Only for VSCode, Codium doesn't have built-in telemetry
    "telemetry.telemetryLevel": "off",
    // Nothing against Red Hat, but I'd rather not use this at work
    "redhat.telemetry.enabled": false,
    //#endregion
    //#region Editor settings
    "window.titleBarStyle": "custom",
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "editor.fontFamily": "'FiraCode Nerd Font', monospace",
    "editor.lineNumbers": "relative",
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.minimap.enabled": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    // Custom zen mode
    "zenMode.hideLineNumbers": false,
    "zenMode.hideStatusBar": false,
    "zenMode.fullScreen": false,
    //#endregion
    //#region Vim emulation settings
    "vim.normalModeKeyBindingsNonRecursive": [
        // Motion between groups using <C-h> and <C-l>
        {
            "before": [
                "<C-l>"
            ],
            "commands": [
                "workbench.action.focusNextGroup"
            ]
        },
        {
            "before": [
                "<C-h>"
            ],
            "commands": [
                "workbench.action.focusPreviousGroup"
            ]
        },
        // Motion between tabs/buffers with H and L
        {
            "before": [
                "L"
            ],
            "commands": [
                "workbench.action.nextEditorInGroup"
            ]
        },
        {
            "before": [
                "H"
            ],
            "commands": [
                "workbench.action.previousEditorInGroup"
            ]
        },
        {
            "before": [
                "]",
                "b"
            ],
            "commands": [
                "workbench.action.nextEditorInGroup"
            ]
        },
        {
            "before": [
                "[",
                "b"
            ],
            "commands": [
                "workbench.action.previousEditorInGroup"
            ]
        },
        // Change view width and height with ctrl + arrows
        {
            "before": [
                "<C-Up>"
            ],
            "commands": [
                "workbench.action.increaseViewHeight"
            ]
        },
        {
            "before": [
                "<C-Down>"
            ],
            "commands": [
                "workbench.action.decreaseViewHeight"
            ]
        },
        {
            "before": [
                "<C-Right>"
            ],
            "commands": [
                "workbench.action.increaseViewWidth"
            ]
        },
        {
            "before": [
                "<C-Left>"
            ],
            "commands": [
                "workbench.action.decreaseViewWidth"
            ]
        },
        // Find actions
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "workbench.action.findInFiles"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "s",
            ],
            "commands": [
                "workbench.action.gotoSymbol"
            ]
        },
        {
            "before": [
                "<leader>",
                "<Space>"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        // Workbench navigation
        {
            "before": [
                "<leader>",
                "e"
            ],
            "commands": [
                "workbench.files.action.focusFilesExplorer"
            ],
            "when": "editorTextFocus"
        },
        {
            "before": [
                "<leader>",
                "s",
                "f"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "<leader>",
                "s",
                "s"
            ],
            "commands": [
                "workbench.action.files.saveFiles"
            ]
        },
        // Close actions
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.closeActiveEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "q",
                "q"
            ],
            "commands": [
                "workbench.action.closeWindow"
            ]
        },
        // Formatting
        {
            "before": [
                "<leader>",
                ";"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        // Code actions
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "s"
            ],
            "commands": [
                "editor.action.sourceAction"
            ]
        },
        {
            "before": [
                "<leader>",
                "r"
            ],
            "commands": [
                "editor.action.rename"
            ]
        },
        // Editor folding
        {
            "before": [
                "<leader>",
                "h",
                "]"
            ],
            "commands": [
                "editor.fold"
            ],
            "when": "editorTextFocus && foldingEnabled"
        },
        {
            "before": [
                "<leader>",
                "h",
                "["
            ],
            "commands": [
                "editor.unfold"
            ],
            "when": "editorTextFocus && foldingEnabled"
        },
        // Java related stuff
        {
            "before": [
                "<leader>",
                "j",
                "c"
            ],
            "commands": [
                "java.view.menus.file.newJavaClass"
            ]
        },
        {
            "before": [
                "<leader>",
                "j",
                "t",
            ],
            "commands": [
                "java.test.goToTest"
            ]
        },
        {
            "before": [
                "<leader>",
                "j",
                "r"
            ],
            "commands": [
                "java.debug.runJavaFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "j",
                "d"
            ],
            "commands": [
                "java.debug.debugJavaFile"
            ]
        },
        // Debugging
        {
            "before": [
                "<leader>",
                "d"
            ],
            "commands": [
                "workbench.action.debug.start"
            ],
            "when": "debuggersAvailable && debugState == inactive"
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "editor.debug.action.toggleBreakpoint"
            ],
            "when": "debuggersAvailable && disassemblyViewFocus || debuggersAvailable && editorTextFocus"
        },
        {
            "description": "Vai para pr√≥ximo marcador (info, erro, warning)",
            "before": [
                "]",
                "m"
            ],
            "commands": [
                "editor.action.marker.next"
            ]
        },
        {
            "before": [
                "[",
                "m"
            ],
            "commands": [
                "editor.action.marker.prev"
            ]
        },
        // Other
        {
            "before": [
                "<leader>",
                "v",
                "z"
            ],
            "commands": [
                "workbench.action.toggleZenMode"
            ]
        },
        {
            "before": [
                "<leader>",
                "t"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                "editor.action.showHover"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "c",
                "a"
            ],
            "commands": [
                "editor.action.quickFix"
            ]
        },
    ],
    "vim.leader": "<Space>",
    "vim.foldfix": true,
    "vim.showMarksInGutter": true,
    //#endregion
    //#region Git stuff
    "git.autofetch": true,
    //#endregion
    //#region Java stuff
    "java.completion.favoriteStaticMembers": [
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*",
        "io.restassured.RestAssured.*",
        "io.restassured.matcher.RestAssuredMatchers.*",
        "org.hamcrest.Matchers.*"
    ],
    "java.maven.updateSnapshots": true,
    "quarkus.tools.alwaysShowWelcomePage": false,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "java.inlayHints.parameterNames.enabled": "all",
    //#endregion
    //#region Terminal and shell stuff
    "terminal.integrated.defaultProfile.linux": "zsh",
    //#endregion
    "window.zoomLevel": 1,
    "workbench.iconTheme": "icons",
    "workbench.colorTheme": "To on Dark",
}